      "Bash(test:*)",
      "Bash(timeout 5 ./node_modules/electron/dist/electron.exe:*)",
      "Bash(npm run build:icon:*)",
      "Bash(npx tsc:*)",
      "Bash(git config:*)",
      "Bash(gh auth status:*)"
